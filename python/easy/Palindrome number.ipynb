   "source": [
    "def isPalindrome(x: int) -> bool:\n",
    "    string_x = str(x)\n",
    "    mid_point = int(len(string_x) / 2)\n",
    "    for i in range(0, mid_point+1):\n",
    "        if string_x[i] != string_x[-i-1]:\n",
    "            return False\n",
    "    return True"
   ]
